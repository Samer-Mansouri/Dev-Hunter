        })\n        .catch(err => {\n            console.log(error.message)\n        })\n    }\n\n    useEffect(() => {\n        fetchPosts();\n        \n    }, [])\n\n  if(loading){\n      return <Loader />\n  } else {\n      return(\n        <div className=\"container mt-2 mb-5 pt-5\">\n        {\n            role == 'dev' ?\n            <AddPost />\n            : \n            ''\n        }\n        <h2 className=\"h2-style mb-4 mt-5\">Projects</h2>\n        <div className=\"row\">\n        {\n            posts.map((post, index) => {\n                return (\n                    \n                  <Fragment key ={index}>\n                    <SinglePost \n                    id={post.id}\n                    title={post.title}\n                    description={post.description}\n                    github_url = {post.github_url}\n                    project_pic = {post.project_pic}\n                    user = {post.user}\n                    username = {post.username}\n                    />\n                  </Fragment>\n                  \n                )\n              })\n\n        }\n        </div>\n    </div>\n      )\n  }\n  \n}\n\nexport default PostsList"},"lineCount":null}},"error":null,"hash":"a5aced3e5f687a803121320577814dd0","cacheData":{"env":{}}}