stsForUser.js","original":{"line":58,"column":36}},{"name":"username","generated":{"line":94,"column":23},"source":"src/Pages/Posts/PostListsForUser.js","original":{"line":58,"column":37}},{"generated":{"line":95,"column":0},"source":"src/Pages/Posts/PostListsForUser.js","original":{"line":51,"column":20}},{"generated":{"line":95,"column":8},"source":"src/Pages/Posts/PostListsForUser.js","original":{"line":50,"column":18}},{"generated":{"line":95,"column":9},"source":"src/Pages/Posts/PostListsForUser.js","original":{"line":48,"column":16}},{"generated":{"line":96,"column":0},"source":"src/Pages/Posts/PostListsForUser.js","original":{"line":63,"column":15}},{"generated":{"line":96,"column":5},"source":"src/Pages/Posts/PostListsForUser.js","original":{"line":47,"column":12}},{"generated":{"line":96,"column":6},"source":"src/Pages/Posts/PostListsForUser.js","original":{"line":45,"column":12}},{"generated":{"line":96,"column":7},"source":"src/Pages/Posts/PostListsForUser.js","original":{"line":38,"column":8}},{"generated":{"line":96,"column":8},"source":"src/Pages/Posts/PostListsForUser.js","original":{"line":37,"column":6}},{"generated":{"line":97,"column":0},"source":"src/Pages/Posts/PostListsForUser.js","original":{"line":72,"column":3}},{"generated":{"line":98,"column":0},"source":"src/Pages/Posts/PostListsForUser.js","original":{"line":74,"column":1}},{"name":"PostListsForUser","generated":{"line":100,"column":15},"source":"src/Pages/Posts/PostListsForUser.js","original":{"line":76,"column":15}}],"sources":{"src/Pages/Posts/PostListsForUser.js":"import React, { Fragment, useEffect, useState } from 'react'\nimport axios from 'axios'\nimport { refreshAccessToken } from '../../Api/RefreshToken';\nimport Loader from '../../Components/Loader';\nimport SinglePostUser from './SinglePostUser';\nfunction PostListsForUser(props) {\n\n    const {id} = props;\n    const [posts, setPosts] = useState([])\n    const [loading, setLoading] = useState(true)\n    const [size, setSize] = useState(5)\n\n    const fetchPosts = () => {\n    \n        axios.get(`http://127.0.0.1:8000/api/posts/userposts/${id}`)\n        .then(res => {\n            console.log(res.data)\n            setPosts(res.data)\n            if(res.data.length <=0){\n                setSize(0)\n            }\n            setLoading(false)\n        })\n        .catch(err => {\n            console.log(error.message)\n        })\n    }\n\n    useEffect(() => {\n        fetchPosts();\n        \n    }, [])\n\n  if(loading){\n      return <Loader />\n  } else {\n      return(\n        <div className=\"container mt-2 mb-5 pt-2\">\n        <h2 className=\"h2-style mb-4 mt-5\">Projects</h2>\n        {\n            size <= 0 ?\n\n            <h3 className=\"h3-style mt-3 text-center\">NO PROJECTS</h3>\n            :\n            <div className=\"row\">\n        {\n            posts.map((post, index) => {\n                return (\n                    \n                  <Fragment key ={index}>\n                    <SinglePostUser\n                    id={post.id}\n                    title={post.title}\n                    description={post.description}\n                    github_url = {post.github_url}\n                    project_pic = {post.project_pic}\n                    user = {post.user}\n                    username = {post.username}\n                    />\n                  </Fragment>\n                  \n                )\n              })\n\n        }\n        </div>\n    \n        }\n        \n    </div>\n      )\n  }\n  \n}\n\nexport default PostListsForUser"},"lineCount":null}},"error":null,"hash":"80b8babc42f31a17c1a534eeeea44221","cacheData":{"env":{}}}